{
  "name": "astro-v5-template",
  "type": "module",
  "version": "0.0.11",
  "homepage": "https://astrov5.casoon.dev/",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "format:svelte": "prettier --write \"**/*.svelte\" --config .prettierrc.svelte.json",
    "format:astro": "prettier --write \"**/*.astro\" --config .prettierrc.astro.json",
    "check:format:svelte": "prettier --check \"**/*.svelte\" --config .prettierrc.svelte.json",
    "check:format:astro": "prettier --check \"**/*.astro\" --config .prettierrc.astro.json",
    "check:biome": "biome check --write .",
    "check": "npm run check:biome && npm run check:format:svelte && npm run check:format:astro",
    "type-check": "astro check"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.3",
    "@astrojs/svelte": "^7.1.0",
    "@fontsource/inter": "^5.2.6",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "aos": "^2.3.4",
    "clsx": "^2.1.1",
    "gsap": "^3.13.0",
    "lucide-react": "^0.525.0",
    "motion": "^12.23.3",
    "svelte": "^5.35.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@types/node-fetch": "^2.6.12",
    "astro": "^5.12.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.4.0",
    "wait-on": "^8.0.3"
  }
}
